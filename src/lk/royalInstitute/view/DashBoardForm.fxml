                                <graphic>
                                    <ImageView fitHeight="55.0" fitWidth="55.0">
                                        <image>
                                            <Image url="@../assets/icons8-accountant-48.png"/>
                                        </image>
                                    </ImageView>
                                </graphic>
                            </JFXButton>
                        </children>
                    </VBox>
                </children>
            </Pane>
            <Pane prefHeight="40.0" prefWidth="1085.0" style="-fx-background-color: #dcdde1;">
