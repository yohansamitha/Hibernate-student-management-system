                    <Label layoutX="488.0" layoutY="8.0" text="Royal institute">
                        <font>
                            <Font name="System Bold" size="16.0"/>
                        </font>
                    </Label>
                </children>
            </Pane>
            <AnchorPane fx:id="context" layoutX="195.0" layoutY="40.0" prefHeight="600.0" prefWidth="890.0"
