                                    <ImageView fitHeight="55.0" fitWidth="55.0">
                                        <image>
                                            <Image url="@../assets/icons8-add-user-male-48.png"/>
                                        </image>
                                    </ImageView>
                                </graphic>
                                <VBox.margin>
                                    <Insets/>
                                </VBox.margin>
                            </JFXButton>
                            <JFXButton onAction="#btnAddRegistrationOnAction" prefWidth="190.0" text="Registration">
                                <graphic>
                                    <ImageView fitHeight="55.0" fitWidth="55.0">
                                        <image>
                                            <Image url="@../assets/icons8-create-64.png"/>
